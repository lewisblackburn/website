    "@zxffo/mdx": "workspace:*",
    "@zxffo/ui": "workspace:*",
    "@zxffo/utils": "workspace:*",
    "cobe": "^0.6.3",
    "dayjs": "^1.11.10",
    "framer-motion": "^11.0.6",
    "geist": "^1.2.2",
    "gray-matter": "^4.0.3",
    "jiti": "^1.21.0",
    "lucide-react": "^0.331.0",
    "next": "14.1.1-canary.44",
    "next-auth": "^5.0.0-beta.13",
    "next-themes": "^0.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
