    "@types/nodemailer": "^6.4.14",
    "@types/react": "^18.2.58",
    "@types/react-dom": "^18.2.19",
    "@types/rss": "^0.0.32",
    "@types/ws": "^8.5.10",
    "@zxffo/eslint-config": "workspace:*",
    "@zxffo/tailwind-config": "workspace:*",
    "@zxffo/tsconfig": "workspace:*",
    "chokidar": "^3.6.0",
    "concurrently": "^8.2.2",
    "drizzle-kit": "^0.20.14",
    "eslint": "^8.57.0",
